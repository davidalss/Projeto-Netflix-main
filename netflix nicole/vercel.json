{
  "version": 2,
  "builds": [
    {
      "src": "netflix nicole/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/netflix nicole/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/netflix nicole/:path*"
    }
  ]
}
